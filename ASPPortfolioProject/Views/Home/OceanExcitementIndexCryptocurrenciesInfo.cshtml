@model List<FullCryptocurrencyInfo>
@{
	var BTC = @Model.Where(i => i.Symbol == "BTC").FirstOrDefault();
	var ETH = @Model.Where(i => i.Symbol == "ETH").FirstOrDefault();
	var USDT = @Model.Where(i => i.Symbol == "USDT").FirstOrDefault();
	var USDC = @Model.Where(i => i.Symbol == "USDC").FirstOrDefault();
	var BNB = @Model.Where(i => i.Symbol == "BNB").FirstOrDefault();
	var ADA = @Model.Where(i => i.Symbol == "ADA").FirstOrDefault();
	var XRP = @Model.Where(i => i.Symbol == "XRP").FirstOrDefault();
	var BUSD = @Model.Where(i => i.Symbol == "BUSD").FirstOrDefault();
	var SOL = @Model.Where(i => i.Symbol == "SOL").FirstOrDefault();
	var DOGE = @Model.Where(i => i.Symbol == "DOGE").FirstOrDefault();
}

<div class="crypto-top-grid text-light" style="margin-left:auto;" id="crypto">
	<div class="crypto-top-block">
		<h3 class="crypto-top-element"><img src="~/icons/bitcoin-icon.png" alt="BTC-icon" style="width:10%; margin-bottom:7px;"> @BTC.Symbol <pre>@BTC.Price$</pre></h3>
		<h3 class="crypto-top-element"><img src="~/icons/eth-icon.png" alt="ETH-icon" style="width:10%; margin-bottom:7px;"> @ETH.Symbol <pre>@ETH.Price$</pre></h3>
	</div>
	<div class="crypto-top-block">
		<h3 class="crypto-top-element"> <img src="~/icons/usdt-icon.png" alt="USDT-icon" style="width:10%; margin-bottom:7px;"> @USDT.Symbol <pre>@USDT.Price$</pre></h3>
		<h3 class="crypto-top-element"> <img src="~/icons/usdc-icon.png" alt="USDC-icon" style="width:10%; margin-bottom:7px;"> @USDC.Symbol <pre>@USDC.Price$</pre></h3>
	</div>
	<div class="crypto-top-block">
		<h3 class="crypto-top-element"> <img src="~/icons/bnb-icon.png" alt="BNB-icon" style="width:10%; margin-bottom:7px;"> @BNB.Symbol <pre>@BNB.Price$</pre></h3>
		<h3 class="crypto-top-element"> <img src="~/icons/ada-icon.png" alt="ADA-icon" style="width:10%; margin-bottom:7px;"> @ADA.Symbol <pre>@ADA.Price$</pre></h3>
	</div>
	<div class="crypto-top-block">
		<h3 class="crypto-top-element"> <img src="~/icons/xrp-icon.png" alt="XRP-icon" style="width:10%; margin-bottom:7px;"> @XRP.Symbol <pre>@XRP.Price$</pre></h3>
		<h3 class="crypto-top-element"> <img src="~/icons/busd-icon.png" alt="BUSD-icon" style="width:10%; margin-bottom:7px;"> @BUSD.Symbol<pre>@BUSD.Price$</pre></h3>
	</div>
	<div class="crypto-top-block">
		<h3 class="crypto-top-element"> <img src="~/icons/sol-icon.png" alt="SOL-icon" style="width:10%; margin-bottom:7px;"> @SOL.Symbol <pre>@SOL.Price$</pre></h3>
		<h3 class="crypto-top-element"> <img src="~/icons/doge-icon.png" alt="DOGE-icon" style="width:10%; margin-bottom:7px;"> @DOGE.Symbol <pre>@DOGE.Price$</pre></h3>
	</div>
